{
  "name": "dioe_app",
  "version": "0.0.2",
  "description": "A desktop client that contains the most important tools used in for research project DIÃ–",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-mac": "electron-packager . dioe_app --platform darwin --arch x64 --icon=app-icon.icns --out ../build-mac --overwrite=true",
    "build": "electron-packager . dioe_app --platform win32 --arch x64 --icon=app-icon.ico --out ../build --asar=true --overwrite=true",
	"setup": "electron-installer-windows --src ../build/dioe_app-win32-x64/ --dest ../installers",
	"build-win": "electron-packager . dioe_app --platform=win32 --arch=ia32 --electronVersion=1.4.13 --app-version=0.0.3 --build-version=1.0.0 --icon=app-icon.ico --prune=false --overwrite=true --asar=true && electron ./package.js"
  },
  "devDependencies": {
    "electron-debug": "^1.0.1",
    "electron-prebuilt": "^1.2.0",
    "electron-winstaller": "^2.5.2",
    "gulp": "^3.9.1",
    "jquery": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/german-in-austria/VRE.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/german-in-austria/VRE/issues"
  },
  "homepage": "https://github.com/german-in-austria/VRE#readme",
  "dependencies": {
    "auto-launch": "^5.0.1",
    "coffee-script": "^1.10.0",
    "electron-connect": "^0.4.2",
    "electron-debug": "^1.0.1",
    "electron-default-menu": "^1.0.0",
    "electron-drag": "^1.2.0",
    "electron-stylus": "^0.1.1",
    "jquery": "^3.1.1",
    "mattermost": "^3.4.0",
    "request-promise": "^4.1.1",
    "tray-windows": "^1.0.1"
  }
}
